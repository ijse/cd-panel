{
  "name": "cd-panel",
  "description": "a Continue Deploy panel",
  "version": "0.0.1",
  "author": "Li Yi <i@ijser.cn>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run dev:server 2>&1 & npm run dev:client",
    "dev:server": "nodemon -w ./server server/start.js",
    "dev:client": "parcel watch --public-url / client/index.html",
    "build": "parcel build --public-url / client/index.html",
    "test": "npm run test:server",
    "test:server": "rimraf .test && NODE_CONFIG_ENV=test mocha server/**/*test.js",
    "clean": "rimraf .cache dist .test"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^5.0.0",
    "parcel-bundler": "parcel-bundler/parcel#vue",
    "power-assert": "^1.4.4",
    "rimraf": "^2.6.2",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^14.0.9",
    "axios": "^0.17.1",
    "config": "^1.29.4",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "nodemon": "^1.14.12",
    "promise.prototype.finally": "^3.1.0",
    "shelljs": "^0.8.1"
  }
}
