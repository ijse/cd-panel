{
  "name": "cd-panel",
  "description": "a Continue Deploy panel",
  "version": "0.0.26",
  "author": "Li Yi <i@ijser.cn>",
  "license": "MIT",
  "scripts": {
    "start": "npm run start:prod",
    "start:lt": "lt -h http://local.ijser.cn -p 3000 -s devcdpanel",
    "start:prod": "forever start ./server/start.js -l ./logs/log.log -e ./logs/error.log -o ./logs/output.log",
    "stop": "forever stop ./server/start.js",
    "restart": "npm stop && npm start",
    "postinstall": "ln -sf ../ node_modules/app || true",
    "dev": "npm run dev:server 2>&1 & npm run dev:client",
    "dev:server": "nodemon -w ./server server/start.js -- --debug",
    "dev:client": "parcel watch --public-url / client/index.html",
    "build": "npm run clean && parcel build --public-url / client/index.html",
    "test": "npm run test:pre && npm run test:server",
    "test-only": "npm run test:pre && npm run mocha --",
    "mocha": "NODE_CONFIG_ENV=test mocha",
    "test:pre": "rimraf .test && npm run mocha -- test.js",
    "test:server": "npm run mocha -- server/**/*test.js",
    "clean": "rimraf .cache dist .test",
    "prepublish": "npm run build",
    "release": "echo release to ${TARGET} ok"
  },
  "bin": {
    "cd-panel": "./server/start.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^5.0.0",
    "parcel-bundler": "parcel-bundler/parcel#vue",
    "power-assert": "^1.4.4",
    "rimraf": "^2.6.2",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^14.0.9",
    "ansi-to-html": "^0.6.4",
    "axios": "^0.17.1",
    "config": "^1.29.4",
    "forever": "^0.15.3",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-morgan": "^1.0.1",
    "koa-passport": "^4.0.1",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "localtunnel": "^1.8.3",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "moment": "^2.20.1",
    "nodemon": "^1.14.12",
    "postcss-import": "^11.1.0",
    "postcss-nesting": "^4.2.1",
    "promise.prototype.finally": "^3.1.0",
    "shelljs": "^0.8.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "vue-socket.io": "^2.1.1-b"
  }
}
